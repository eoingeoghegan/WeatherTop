{{#each stations }}

<div class="box box-link-hover-shadow">
  <h2 class="title">
    {{title}}
    {{latitude}}
    {{longitude}}
    
  </h2>
  <a href="/station/{{_id}}" class="button is-primary">
    {{>icons/open}}
  </a>
  <a href="/dashboard/deletestation/{{_id}}" class="button is-danger">
    {{> icons/delete}}
  </a>
</div>
{{/each}}